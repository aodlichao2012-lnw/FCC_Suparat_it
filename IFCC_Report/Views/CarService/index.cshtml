
@{
    ViewBag.Title = "Car Service Report";
    ViewBag.Controller = "CarServiceController";
    Layout = "~/Views/Shared/_Layout.cshtml";
    //ViewBag.activeMain = "Master";
    //ViewBag.active = "Student";

}
@section javascripts{
    <script src="@Url.Content("~/Assets/js/controllers/carServiceController.js?v=")"></script>

}
<section id="main-content" vertilize-container>
    <section class="wrapper">
        <h3><i class="fa fa-cogs"></i> Car Service Report</h3>
        <div class="row mt">
            <div class="col-lg-12 no-padding no-margin">
                <div class="form-panel no-margin-t">
                    <form class="form-horizontal tasi-form">
                        <div class="form-group no-margin">
                            <div class="col-sm-6">
                            </div>
                            <div></div>

                            <div class="col-xs-12">

                                <div class="col-md-6">
                                    <label class="col-sm-3 control-label required-label">Date Rage</label>
                                    <div class="col-sm-9">
                                        <div id="request_date_range" class="form-control">
                                            <i class="fa fa-calendar"></i> &nbsp;
                                            <span> </span>
                                            <b class="fa fa-angle-down pull-right"></b>
                                        </div>
                                    </div>
                                </div>
                               

                            </div>
                            <br />
                            <br />
                            <br />
                            <br />
                            <div class="col-xs-12">

                                <div class="right  text-right">


                                    <button type="button" name="Search_data" class="btn btn-theme-gray btn-90 CheckDate" ng-click="baseButtonClick('Search')"><i class="fa fa-search"></i>&nbsp;&nbsp;Search Data</button>

                                    <button type="button" name="btn_Export" class="btn btn-theme-blue btn-90 CheckDate" ng-click="ExportAllExcel()"><i class="fa fa-download"></i>&nbsp;&nbsp;Export</button>

                                  
                                </div>

                            </div>

                        </div>
                    </form>
                </div>
            </div>



        </div>

        <div class="row">
            <div class="col-md-12 no-padding no-margin">
                <div class="form-panel no-margin-t" vertilize>
                    <br />
                    <div class="table table-striped table-condensed" style="overflow:auto;">
                        <h4> Car Service Report </h4>
                        <br />
                        
                        <div class="table table-striped table-condensed" style="overflow:auto;">
                            <div class="col-md-2 text-left">
                                <p><span class="topic"></span></p>
                            </div>

                            <table id="table_list" class="table table-bordered table-striped table-condensed" style="width:100%;">
                                <thead>

                                    <tr>
                                        <th class="text-center" style="width:auto">เลขที่เคส</th>
                                        <th class="text-center" style="width:auto">ประเภทบริการ</th>
                                        <th class="text-center" style="width:auto">ประเภทผลิตภัณฑ์</th>
                                        <th class="text-center" style="width:auto">วันที่แลกสิทธิ์</th>
                                        <th class="text-center" style="width:auto">ชื่อเจ้าของสิทธิ์</th>
                                        <th class="text-center" style="width:auto">เบอร์โทรเจ้าของสิทธิ์</th>
                                        <th class="text-center" style="width:auto">ชื่อผู้ใช้สิทธิ์</th>
                                        <th class="text-center" style="width:auto">เบอร์โทรผู้ใช้สิทธิ์</th>
                                        <th class="text-center" style="width:auto">ที่อยู่</th>
                                        <th class="text-center" style="width:auto">จัดส่งโดย</th>
                                        <th class="text-center" style="width:auto">สถานะจัดส่ง</th>
                                        <th class="text-center" style="width:auto">Tracking Code</th>
                                        <th class="text-center" style="width:auto">Create By</th>
                                        <th class="text-center" style="width:auto">น้องหอมจัง</th>
                                    </tr>
                                </thead>

                            </table>

                        </div>
                        <br />

                        <br />
                    </div>

                </div>
            </div>

        </div>

        @*Log Create & Update*@
       
    </section>
</section>

